<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="FXBusinessLogic.Repo" namespace="FXBusinessLogic.Repo" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Techindicator" table="techindicator" lazy="true" >
    <id name="Id" column="ID" />
    <property name="Indicatorname">
      <column name="IndicatorName" sql-type="varchar(50)" not-null="true" />
    </property>
    <property name="Enabled">
      <column name="Enabled" sql-type="tinyint(1)" not-null="false" />
    </property>
  </class>
</hibernate-mapping>